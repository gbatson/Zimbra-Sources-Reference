			<td align='right' style='padding-right:5px;'>
				<b><div style='white-space:nowrap'><$= AjxStringUtil.htmlEncode(ZmMsg.locationLabel) $></div></b>
			</td>
			<td align='left'>
				<div style='white-space:nowrap;'><$= AjxStringUtil.htmlEncode(data.location) $></div>
			</td>
		</tr>
	<$ } $>

