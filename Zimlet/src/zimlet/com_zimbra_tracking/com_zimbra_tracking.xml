<zimlet name="com_zimbra_tracking" version="1.2" description="Find shipping information for FedEx and UPS using the tracking number.">
	<include>shipping.js</include>
	<handlerObject>Com_Zimbra_Tracking</handlerObject>
	<serverExtension hasKeyword="tracking" extensionClass="com.zimbra.cs.zimlet.handler.TrackingHandler"/>
	<contentObject type="tracking">
		<matchOn>
			<regex>Overrode in shipping.js</regex>
		</matchOn>
	</contentObject>
</zimlet>
